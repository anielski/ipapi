{
    "name": "adamn/ipapi",
    "description": "This is the test.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "api",
        "test",
        "website",
        "application",
        "microservices"
    ],
    "authors": [
        {
            "name": "Adam Nielski",
            "email": "adam.nielski@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "^6.3",
        "fzaninotto/faker": "^1.7",
        "ivanovpvl/ip-api-php": "0.2.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\Package\\": "src/",
            "tests\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "src/",
            "tests\\": "tests"
        }
    },
    "require-dev": {
        "elementaryframework/water-pipe": "dev-master",
        "phpunit/phpunit": "^6.5"
    },
    "scripts": {
        "test": [
            "@phpunit"
        ],
        "phpunit": "php vendor/bin/phpunit"
    },
    "repositories":[
        {
            "type": "package",
            "package": {
                "name": "ivanovpvl/ip-api-php",
                "version": "1.0",
                "source": {
                    "url": "https://github.com/ivpal/ip-api-php.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ]
}
